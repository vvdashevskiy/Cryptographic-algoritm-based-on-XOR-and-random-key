<Page x:Class="Cryptographic_algoritm_based_on_XOR_and_random_key.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cryptographic_algoritm_based_on_XOR_and_random_key"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="500"
      Title="MainPage">

    <Grid>
        <Button x:Name="Locked" Margin="0,0,125,0" VerticalAlignment="Center" HorizontalAlignment="Center" Width="100" Height="100" Click="Locked_Click">
            <Image Source="Resources/Locked.png"></Image>
        </Button>
        <Button x:Name="Unlocked"  Margin="125,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" Width="100" Height="100" Click="Unlocked_Click">
            <Image Source="Resources/Unlocked.png"></Image>
        </Button>
    </Grid>
</Page>
